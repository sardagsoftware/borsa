# ECW API - Environment Variables Template (v7.3)
# SECURITY: NEVER commit .env file - this is the template only

# Database
DATABASE_URL="postgresql://user:password@localhost:5432/ecw_db?schema=public"

# Server
PORT=3200
NODE_ENV=development

# JWT & Security
JWT_SECRET=change-this-to-a-secure-random-string-min-32-chars
JWT_EXPIRES_IN=24h
JWKS_KID=ecw-signing-k1

# Ethics & Compliance
ECW_FAILSAFE=strict
ECW_REGION_DEFAULT=EU
RESIDENCY_STRICT=true

# External APIs (v7.3 integrations)
NICO_API_URL=https://api.ailydian.com/v4.6/nico/telemetry/regions
