ESK_OBJECTIVE_ID=omega-ethics-v1
OPA_POLICY_URL=https://opa.ailydian.com/v1/data/ailydian/allow

# Audit & Proof
ARP_EVIDENCE_URL=https://api.ailydian.com/v3.4/arp/evidence/pack
RFC3161_TSA_URL=https://tsa.example.com/timestamp
