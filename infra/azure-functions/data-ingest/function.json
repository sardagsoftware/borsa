{
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "post"
      ],
      "route": "ingest/{dataType}"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "type": "blob",
      "direction": "out",
      "name": "outputBlob",
      "path": "ingestion/{dataType}/{DateTime}.json",
      "connection": "AzureWebJobsStorage"
    }
  ],
  "scriptFile": "index.js"
}
