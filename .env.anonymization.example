# ━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━
# PROVIDER D CONFIGURATION (Optional - Additional Provider)
# ━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━
PROVIDER_D_ENDPOINT=https://api.example-d.com/v1/chat
PROVIDER_D_API_KEY=example-key-d

# Model mappings for Provider D
MODEL_FAST_D=example-model-fast-d
MODEL_ADVANCED_D=example-model-advanced-d

# ━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━
# FALLBACK & LOAD BALANCING
# ━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━

# Primary provider priority (comma-separated hashed IDs)
# In production, this would be hashed provider IDs
PRIMARY_PROVIDER=provider_a
SECONDARY_PROVIDER=provider_b
TERTIARY_PROVIDER=provider_c

# Automatic failover (true/false)
ENABLE_FAILOVER=true

# Load balancing strategy (round_robin, least_cost, least_latency)
LOAD_BALANCING_STRATEGY=least_cost

# ━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━
# SECURITY & MONITORING
# ━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━

# Enable security guards (true/false)
ENABLE_HEADER_GUARD=true
ENABLE_EGRESS_GUARD=true
ENABLE_LOG_REDACTION=true

# Egress guard mode (monitor/enforce)
EGRESS_GUARD_MODE=enforce

# Log redaction minimum severity (low/medium/high/critical)
LOG_REDACTION_MIN_SEVERITY=high

# Alert webhook for security events
SECURITY_ALERT_WEBHOOK=

# ━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━
# USAGE TRACKING (Provider-Agnostic)
# ━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━

# Track usage by internal model ID only (never log provider names)
ENABLE_USAGE_TRACKING=true
USAGE_TRACKING_ENDPOINT=

# Cost tracking (optional - uses generic cost tiers)
ENABLE_COST_TRACKING=true

# ━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━
# DEVELOPMENT ONLY (Remove in production)
# ━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━

# Enable debug logging (NEVER enable in production)
DEBUG_MODE=false

# Mock provider for testing (NEVER enable in production)
ENABLE_MOCK_PROVIDER=false
