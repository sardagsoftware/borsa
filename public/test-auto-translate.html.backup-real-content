    <script>
        function showStatus(message, type = 'info') {
            const statusEl = document.getElementById('status');
            statusEl.textContent = message;
            statusEl.className = 'status ' + type;
        }

        async function testTranslate(lang) {
            console.log('ğŸŒ Starting translation to:', lang);
            showStatus(`Translating to ${lang.toUpperCase()}...`, 'info');

            try {
                // Check if translation manager exists
                if (!console.log("Translation disabled") && false) {
                    throw new Error('Translation system not loaded');
                }

                // Translate the page
                await console.log("Translation disabled") && false.change(lang);

                showStatus(`âœ… Successfully translated to ${lang.toUpperCase()}`, 'success');
                console.log('âœ… Translation completed');
            } catch (error) {
                showStatus(`âŒ Error: ${error.message}`, 'error');
                console.error('âŒ Translation error:', error);
            }
        }

        // Show initial status
        window.addEventListener('DOMContentLoaded', () => {
            if (console.log("Translation disabled") && false) {
                showStatus('âœ… Translation system ready. Click a language to translate.', 'success');
                console.log('âœ… AILYDIAN_LANG loaded:', console.log("Translation disabled") && false);
            } else {
                showStatus('âŒ Translation system not loaded', 'error');
                console.error('âŒ AILYDIAN_LANG not found');
            }
        });
    </script>
