    <script>
        async function testChatAPI() {
            const resultDiv = document.getElementById('result');
            resultDiv.innerHTML = 'â³ Testing API...';

            try {
                console.log('ğŸ” Sending request to /api/chat/specialized...');

                const response = await fetch('/api/chat/specialized', {
                    method: 'POST',
                    headers: {
                        'Content-Type': 'application/json',
                        'Accept-Language': 'tr-TR',
                        'Content-Language': 'tr'
                    },
                    body: JSON.stringify({
                        message: 'Merhaba, test mesajÄ±',
                        aiType: 'general',
                        temperature: 0.7,
                        max_tokens: 2048,
                        history: [],
                        language: 'tr',
                        locale: 'tr-TR'
                    })
                });

                console.log('ğŸ“¡ Response status:', response.status);
                console.log('ğŸ“¡ Response ok:', response.ok);
                console.log('ğŸ“¡ Response headers:', [...response.headers.entries()]);

                if (!response.ok) {
                    throw new Error(`HTTP ${response.status}: ${response.statusText}`);
                }

                const data = await response.json();
                console.log('âœ… Response data:', data);

                resultDiv.innerHTML = `<div class="success">âœ… SUCCESS!</div>\n\n` +
                    `Status: ${response.status}\n` +
                    `Provider: ${data.provider}\n` +
                    `AI Type: ${data.aiType}\n\n` +
                    `Response:\n${data.response}\n\n` +
                    `Full JSON:\n${JSON.stringify(data, null, 2)}`;

            } catch (error) {
                console.error('âŒ Error:', error);
                resultDiv.innerHTML = `<div class="error">âŒ ERROR!</div>\n\n` +
                    `Message: ${error.message}\n` +
                    `Stack: ${error.stack}`;
            }
        }
    </script>
