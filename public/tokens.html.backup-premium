    <script>
        // Real-time data updates
        function updateRealTimeData() {
            // Simulate real-time updates
            const updates = {
                totalTokens: Math.floor(Math.random() * 1000000) + 9000000,
                burnedTokens: Math.floor(Math.random() * 500000) + 2000000,
                tokenPrice: (Math.random() * 0.1 + 0.2).toFixed(3),
                activeUsers: Math.floor(Math.random() * 5000) + 15000,
                dailyConsumed: Math.floor(Math.random() * 10000) + 40000,
                activeTransactions: Math.floor(Math.random() * 500) + 1000,
                hashRate: (Math.random() * 2 + 97).toFixed(1),
                gasFee: Math.floor(Math.random() * 10) + 8,
                marketCap: (Math.random() * 1 + 2).toFixed(1),
                volume: Math.floor(Math.random() * 50) + 130
            };

            Object.keys(updates).forEach(key => {
                const element = document.getElementById(key);
                if (element) {
                    if (key === 'tokenPrice') {
                        element.textContent = '$' + updates[key];
                    } else if (key === 'hashRate') {
                        element.textContent = updates[key] + '%';
                    } else if (key === 'gasFee') {
                        element.textContent = updates[key] + ' Gwei';
                    } else if (key === 'marketCap') {
                        element.textContent = '$' + updates[key] + 'M';
                    } else if (key === 'volume') {
                        element.textContent = '$' + updates[key] + 'K';
                    } else {
                        element.textContent = updates[key].toLocaleString();
                    }
                }
            });
        }

        // Update data every 3 seconds
        setInterval(updateRealTimeData, 3000);

        // Initial update
        updateRealTimeData();

        // Button click handlers
        document.querySelectorAll('.tier-button').forEach(button => {
            button.addEventListener('click', function() {
                const tierName = this.closest('.tier-card').querySelector('.tier-name').textContent;
                alert(`${tierName} paketi iÃ§in iÅŸlem baÅŸlatÄ±lÄ±yor...`);
            });
        });
    </script>
    <script src="/js/mobile-menu-handler.js"></script>
</body>
