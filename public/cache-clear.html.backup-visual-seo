    <div class="card">
        <h1>üîÑ Cache Temizleme</h1>
        <p class="warning">Deƒüi≈üiklikler g√∂r√ºnm√ºyorsa cache'i temizleyin:</p>
        
        <h3>Manuel Temizleme:</h3>
        <ol>
            <li><strong>Chrome/Edge:</strong> Cmd + Shift + R (Mac) veya Ctrl + Shift + R (Windows)</li>
            <li><strong>Safari:</strong> Cmd + Option + E (Cache temizle) sonra Cmd + R</li>
            <li><strong>Firefox:</strong> Cmd + Shift + R (Mac) veya Ctrl + F5 (Windows)</li>
        </ol>

        <h3>Otomatik Temizleme:</h3>
        <button onclick="clearCache()">üóëÔ∏è Cache Temizle ve Yenile</button>
        <button onclick="clearServiceWorkers()">‚öôÔ∏è Service Workers Temizle</button>
        <button onclick="goToLydianIQ()">üöÄ Lydian IQ'ya Git</button>

        <div id="status" style="margin-top: 20px;"></div>
    </div>

    <script>
        function showStatus(msg, isSuccess = true) {
            document.getElementById('status').innerHTML = 
                `<p class="${isSuccess ? 'success' : 'warning'}">${msg}</p>`;
        }

        async function clearCache() {
            try {
                // Clear all caches
                if ('caches' in window) {
                    const cacheNames = await caches.keys();
                    await Promise.all(cacheNames.map(name => caches.delete(name)));
                    showStatus('‚úÖ Cache temizlendi! Sayfa yenileniyor...');
                }
                
                // Clear localStorage and sessionStorage
                localStorage.clear();
                sessionStorage.clear();
                
                setTimeout(() => {
                    window.location.reload(true);
                }, 1000);
            } catch (e) {
                showStatus('‚ö†Ô∏è Hata: ' + e.message, false);
            }
        }

        async function clearServiceWorkers() {
            try {
                if ('serviceWorker' in navigator) {
                    const registrations = await navigator.serviceWorker.getRegistrations();
                    await Promise.all(registrations.map(reg => reg.unregister()));
                    showStatus('‚úÖ Service Workers temizlendi! Sayfayƒ± yenileyin.');
                } else {
                    showStatus('‚ö†Ô∏è Service Worker desteklenmiyor', false);
                }
            } catch (e) {
                showStatus('‚ö†Ô∏è Hata: ' + e.message, false);
            }
        }

        function goToLydianIQ() {
            // Add timestamp to force reload
            window.location.href = `/lydian-iq.html?t=${Date.now()}`;
        }

        // Auto-clear on load
        if (window.location.search.includes('auto=1')) {
            clearCache();
        }
    </script>
