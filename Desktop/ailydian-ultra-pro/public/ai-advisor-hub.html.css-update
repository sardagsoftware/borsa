                alert('✅ Başarıyla eklendi! Modül hazır olduğunda bilgilendirileceksiniz.');
                // TODO: API call to save email
            }
        }

        function startQuickSession(type) {
            // Security: Logging disabled
            switch(type) {
                case 'health':
                    launchModule('health-orchestrator');
                    break;
                case 'career':
                    launchModule('life-coach');
                    break;
                case 'decision':
                    launchModule('decision-matrix');
                    break;
                case 'startup':
                    launchModule('startup-accelerator');
                    break;
            }
        }

        // Keyboard shortcuts
        document.addEventListener('keydown', (e) => {
            if (e.ctrlKey || e.metaKey) {
                switch(e.key) {
                    case '1':
                        e.preventDefault();
                        launchModule('health-orchestrator');
                        break;
                    case '2':
                        e.preventDefault();
                        launchModule('decision-matrix');
                        break;
                }
            }
        });

