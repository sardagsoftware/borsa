    <script>
        // Security Protection Layer
        (function() {
            // Disable right-click
            document.addEventListener('contextmenu', e => e.preventDefault());

            // Disable F12, Ctrl+Shift+I, Ctrl+Shift+J, Ctrl+U
            document.addEventListener('keydown', e => {
                if (e.key === 'F12' ||
                    (e.ctrlKey && e.shiftKey && (e.key === 'I' || e.key === 'J' || e.key === 'C')) ||
                    (e.ctrlKey && e.key === 'U')) {
                    e.preventDefault();
                    return false;
                }
            });

            // Console protection
            const noop = () => {};
            ['log', 'debug', 'info', 'warn', 'error', 'trace', 'dir', 'dirxml', 'group', 'groupCollapsed', 'groupEnd', 'clear', 'count', 'countReset', 'assert', 'profile', 'profileEnd', 'time', 'timeLog', 'timeEnd', 'timeStamp', 'table', 'exception'].forEach(method => {
                if (typeof console !== 'undefined' && typeof console[method] !== 'undefined') {
                    console[method] = noop;
                }
            });

            // Detect DevTools
            const devtools = {isOpen: false};
            const element = new Image();
            Object.defineProperty(element, 'id', {
                get: function() {
                    devtools.isOpen = true;
                    if (devtools.isOpen) {
                        window.location.href = 'about:blank';
                    }
                }
            });
            setInterval(() => { console.log(element); }, 1000);
        })();

        // Module Launch Functions
        function launchModule(moduleName) {
            // Removed logging for security

            // Show loading state
            event.target.innerHTML = '<span class="loading"></span> Başlatılıyor...';
            event.target.disabled = true;

            // Simulate API call
            setTimeout(() => {
                switch(moduleName) {
                    case 'health-orchestrator':
                        window.location.href = '/ai-health-orchestrator.html';
                        break;
                    case 'decision-matrix':
                        window.location.href = '/ai-decision-matrix.html';
                        break;
                    case 'meeting-insights':
                        window.location.href = '/ai-meeting-insights.html';
                        break;
                    case 'startup-accelerator':
                        window.location.href = '/ai-startup-accelerator.html';
                        break;
                    case 'life-coach':
                        window.location.href = '/ai-life-coach.html';
                        break;
                    case 'learning-path':
                        window.location.href = '/ai-learning-path.html';
                        break;
                    case 'cultural-advisor':
                        window.location.href = '/ai-cultural-advisor.html';
                        break;
                    case 'knowledge-assistant':
                        window.location.href = '/ai-knowledge-assistant.html';
                        break;
                    default:
                        alert('Modül hazırlanıyor...');
                        event.target.innerHTML = '<i class="fas fa-play"></i> Analiz Başlat';
                        event.target.disabled = false;
                }
            }, 1000);
        }

        function viewDemo(type) {
            alert(`${type} demo videosu yakında eklenecek`);
        }

        function joinWaitlist(module) {
            const email = prompt('E-posta adresinizi girin (bekleme listesine ekleneceksiniz):');
